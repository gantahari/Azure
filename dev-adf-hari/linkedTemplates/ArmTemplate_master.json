{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-adf-hari"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"AzureDataLakeStorageADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageADLS'"
		},
		"AzureSqlDatabase_Connection_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase_Connection'"
		},
		"AzureDataLakeStorageADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devadlshari.dfs.core.windows.net/"
		},
		"AzureKeyVault_Connection_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://devkvsecrectsadfhari.vault.azure.net/"
		},
		"AzureSqlDatabase_Connection_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dev-mssql-server-hari.database.windows.net"
		},
		"AzureSqlDatabase_Connection_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dev-mssql-db-hari"
		},
		"AzureSqlDatabase_Connection_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "hari"
		},
		"midnighttrigger_properties_pipeline1_parameters_env": {
			"type": "string",
			"defaultValue": "dev"
		},
		"midnighttrigger_properties_SQL_ADLS_parameters_env": {
			"type": "string",
			"defaultValue": "dev"
		},
		"default_properties_env_value": {
			"type": "string",
			"defaultValue": "dev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-adf-hari_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_connectionString": {
						"value": "[parameters('AzureBlobStorage_connectionString')]"
					},
					"AzureDataLakeStorageADLS_accountKey": {
						"value": "[parameters('AzureDataLakeStorageADLS_accountKey')]"
					},
					"AzureSqlDatabase_Connection_password": {
						"value": "[parameters('AzureSqlDatabase_Connection_password')]"
					},
					"default_properties_env_value": {
						"value": "[parameters('default_properties_env_value')]"
					},
					"AzureDataLakeStorageADLS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageADLS_properties_typeProperties_url')]"
					},
					"AzureKeyVault_Connection_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_Connection_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase_Connection_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_Connection_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_Connection_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_Connection_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_Connection_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_Connection_properties_typeProperties_userName')]"
					},
					"midnighttrigger_properties_pipeline1_parameters_env": {
						"value": "[parameters('midnighttrigger_properties_pipeline1_parameters_env')]"
					},
					"midnighttrigger_properties_SQL_ADLS_parameters_env": {
						"value": "[parameters('midnighttrigger_properties_SQL_ADLS_parameters_env')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}